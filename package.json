{
    "name": "bpftracer",
    "displayName": "bpftracer",
    "description": "Create and run bpftrace programs from VSCode.",
    "version": "0.0.1",
    "publisher": "vladov3000",
    "repository": "https://github.com/vladov3000/bpftracer",
    "engines": {
        "vscode": "^1.51.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:bpftrace"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "bpftrace.restartLanguageServer",
                "title": "bpftrace: Restart Language Server"
            }
        ],
        "languages": [
            {
                "id": "bpftrace",
                "extensions": [
                    ".bt"
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc --project ./",
        "watch": "tsc --watch --project ./"
    },
    "dependencies": {
        "vscode-languageserver": "^9.0.1",
        "vscode-languageclient": "^9.0.1",
        "vscode-languageserver-textdocument": "^1.0.12"
    },
    "devDependencies": {
        "@types/node": "^22.15.30",
        "@types/vscode": "^1.100.0",
        "typescript": "^5.8.3"
    }
}